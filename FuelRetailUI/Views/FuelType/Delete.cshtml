@model RetailMasters.Models.fueltype

<h2>Confirm Delete</h2>

<h4>Are you sure you want to delete this fuel type?</h4>
<hr />
<dl class="row">
    <dt class="col-sm-2">ID</dt>
    <dd class="col-sm-10">@Model.fueltypeid</dd>

    <dt class="col-sm-2">Name</dt>
    <dd class="col-sm-10">@Model.fueltypename</dd>

    <dt class="col-sm-2">Description</dt>
    <dd class="col-sm-10">@Model.description</dd>
</dl>

<form asp-action="Delete" method="post">
    <input type="hidden" asp-for="fueltypeid" />
    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>
