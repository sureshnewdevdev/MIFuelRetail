@model RetailMasters.Models.valettype

<h2>Confirm Delete</h2>

<h4>Are you sure you want to delete this valet type?</h4>
<hr />
<dl class="row">
    <dt class="col-sm-2">ID</dt>
    <dd class="col-sm-10">@Model.valettypeid</dd>

    <dt class="col-sm-2">Type Name</dt>
    <dd class="col-sm-10">@Model.valettypename</dd>
</dl>

<form asp-action="Delete" method="post">
    <input type="hidden" asp-for="valettypeid" />
    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>
